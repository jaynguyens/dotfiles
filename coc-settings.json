{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "eslint.autoFixOnSave": true,
  "tsserver.formatOnType": true,
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "diagnostic.errorSign": "⎯✗",
  "diagnostic.warningSign": "⎯∆",
  "diagnostic.infoSign": "⎯⏍",
  "diagnostic.hintSign": "⎯✔︎",
  "prettier.disableSuccessMessage": true
}
